<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean
            id="userController"
            class="usts.pycro.spring.controller.UserController"
            autowire="byName"/>


    <bean
            id="userServiceImpl"
            class="usts.pycro.spring.service.impl.UserServiceImpl"
            autowire="byName"/>

    <bean
            id="userService2"
            class="usts.pycro.spring.service.impl.UserServiceImpl"
            autowire="byName"/>

    <bean id="userDaoImpl" class="usts.pycro.spring.dao.impl.UserDaoImpl"/>
    <bean id="userDao2" class="usts.pycro.spring.dao.impl.UserDaoImpl"/>
</beans>